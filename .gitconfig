[alias]
	br = branch
	st = status
  cm = commit
  co = checkout
  d  = diff
  r  = rebase
	unstage = reset HEAD --
	last = log -1 HEAD
  amend = commit --amend
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
